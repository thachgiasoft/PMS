<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="PSCLic.Data" type="PSCLic.Data.Bases.NetTiersServiceSection, PSCLic.Data" allowDefinition="MachineToApplication" restartOnExternalChanges="true" />
  </configSections>
  <connectionStrings>
    <add name="netTiersConnectionString" connectionString="Data Source=ADMIN;Initial Catalog=psclic;User ID=sa;Password=sa" />
  </connectionStrings>
  <PSCLic.Data defaultProvider="SqlNetTiersProvider">
    <providers>
      <add name="SqlNetTiersProvider" type="PSCLic.Data.SqlClient.SqlNetTiersProvider, PSCLic.Data.SqlClient" connectionStringName="netTiersConnectionString" providerInvariantName="System.Data.SqlClient" entityFactoryType="PSCLic.Entities.EntityFactory" useEntityFactory="true" enableEntityTracking="false" enableMethodAuthorization="false" useStoredProcedure="false" />
    </providers>
  </PSCLic.Data>
  <appSettings>
    <!--
      [0] = "Caramel"
      [1] = "Money Twins"
      [2] = "Lilian"
      [3] = "The Asphalt World"
      [4] = "iMaginary"
      [5] = "Black"
      [6] = "Blue"  
      -->
    <add key="SkinID" value="1" />
    <add key="Lang" value="en-US" />
    <add key="FormatNumeric" value="0,0" />
  <add key="Languages" value="" /></appSettings>
<startup><supportedRuntime version="v2.0.50727" /></startup></configuration>
